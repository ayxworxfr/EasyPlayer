    <plurals name="mp_folders_created_formatter" tools:ignore="ExtraTranslation">
        <item quantity="one" tools:ignore="ExtraTranslation">%d new folder created</item>
        <item quantity="other" tools:ignore="ExtraTranslation">%d new folders created</item>
    </plurals>
    <plurals name="mp_directory_items_formatter" tools:ignore="ExtraTranslation">
        <item quantity="one" tools:ignore="ExtraTranslation">%d item</item>
