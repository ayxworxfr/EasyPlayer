    <!-- Main -->
    <string-array name="mp_main_titles" tools:ignore="ExtraTranslation">
        <item>@string/mp.fragment.title.play_list</item>
        <item>@string/mp.fragment.title.video</item>
        <item>@string/mp.fragment.title.local_files</item>
        <item>@string/mp.fragment.title.settings</item>
    </string-array>

